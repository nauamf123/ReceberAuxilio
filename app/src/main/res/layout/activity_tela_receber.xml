<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#7EEDBC"
    tools:context=".TelaReceber">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="0dp"
        android:layout_height="51dp"

        android:layout_marginStart="65dp"
        android:layout_marginTop="108dp"
        android:layout_marginEnd="65dp"
        android:layout_marginBottom="107dp"
        android:text="ReceberAuxílio"
        android:textColor="#080BCD"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/cpf1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txdata"
        android:layout_width="0dp"
        android:layout_height="31dp"

        android:layout_marginStart="20dp"
        android:layout_marginEnd="21dp"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/txdata3"
        app:layout_constraintEnd_toStartOf="@+id/txdata2"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txdata2"
        android:layout_width="0dp"
        android:layout_height="31dp"

        android:layout_marginEnd="36dp"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/txdata4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txdata" />

    <TextView
        android:id="@+id/txdata4"
        android:layout_width="0dp"
        android:layout_height="31dp"

        android:layout_marginEnd="36dp"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/txdata"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txdata3" />

    <TextView
        android:id="@+id/renda1"
        android:layout_width="227dp"
        android:layout_height="33dp"

        android:layout_marginStart="80dp"
        android:layout_marginBottom="283dp"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/cpf1"
        android:layout_width="227dp"
        android:layout_height="39dp"


        android:layout_marginTop="29dp"
        android:layout_marginEnd="9dp"
        android:layout_marginBottom="25dp"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="198dp"
        android:layout_marginBottom="198dp"
        android:text="CPF"
        app:layout_constraintBottom_toTopOf="@+id/renda1"
        app:layout_constraintEnd_toEndOf="@+id/cpf1"
        app:layout_constraintStart_toStartOf="@+id/cpf1"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:layout_marginTop="93dp"
        android:layout_marginBottom="93dp"
        android:text="DataDenascimento"
        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="@+id/txdata"
        app:layout_constraintTop_toTopOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="24dp"
        android:text="DataDeEntrega"
        app:layout_constraintStart_toStartOf="@+id/txdata2"
        app:layout_constraintTop_toBottomOf="@+id/cpf1" />

    <TextView
        android:id="@+id/txdata3"
        android:layout_width="0dp"
        android:layout_height="32dp"

        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="41dp"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintEnd_toStartOf="@+id/txdata4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="84dp"
        android:layout_marginEnd="84dp"
        android:layout_marginBottom="12dp"
        android:text="Receberá"
        app:layout_constraintBottom_toTopOf="@+id/renda1"
        app:layout_constraintEnd_toEndOf="@+id/cpf1"
        app:layout_constraintStart_toStartOf="@+id/renda1" />
</androidx.constraintlayout.widget.ConstraintLayout>